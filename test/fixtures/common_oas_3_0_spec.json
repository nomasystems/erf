{
    "components": {
        "schemas": {
            "Foo": {
                "type": "string",
                "description": "A foo",
                "enum": [
                    "bar",
                    "baz"
                ]
            },
            "Error": {
                "type": "object",
                "properties": {
                    "description": {
                        "type": "string",
                        "description": "An English human-friendly description of the error."
                    }
                }
            }
        },
        "responses": {
            "NoContent": {
                "description": "The request was successful but there is no representation to return."
            },
            "NotFound": {
                "description": "The requested resource was not found.",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Error"
                        }
                    }
                }
            }
        },
        "parameters": {
            "Version": {
                "in": "path",
                "name": "version",
                "schema": {
                    "type": "string",
                    "pattern": "^[0-9]+$"
                },
                "required": true,
                "description": "Version of the API"
            },
            "Enabled": {
                "in": "query",
                "name": "enabled",
                "schema": {
                    "type": "boolean"
                },
                "required": false,
                "description": "Filter by enabled"
            },
            "Page": {
                "in": "query",
                "name": "page",
                "schema": {
                    "type": "integer"
                },
                "required": false,
                "description": "Page number"
            },
            "Price": {
                "in": "query",
                "name": "price",
                "schema": {
                    "type": "number"
                },
                "required": false,
                "description": "Price"
            },
            "Color": {
                "in": "query",
                "name": "color",
                "schema": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "required": false,
                "description": "Color array"
            }
        },
        "requestBodies": {
            "PostFoo": {
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Foo"
                        }
                    }
                }
            }
        }
    }
}
